.................... void justify(char value){ 
....................   int8 pos = 0; 
....................  
....................   switch(toupper(value)) { 
....................     case 'L': pos = 0; break; 
....................     case 'C': pos = 1; break; 
....................     case 'R': pos = 2; break; 
....................   } 
....................  
....................   writeBytes(ASCII_ESC, 'a', pos); 
